pipeline {
    agent any
    stages {
        stage('scm checkout') {
            steps {
                echo 'getting from github'
                echo 'checkout completed'
            }
        }
        stage('build') {
            steps {
                echo 'build completed'
                echo 'new changes'
            }
        }
        stage('test') {
            steps {
                echo 'test completed'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploy completed'
            }
        }
    }    
}
